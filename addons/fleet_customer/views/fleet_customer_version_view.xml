<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="fleet_customer_version_view_form" model="ir.ui.view">
            <field name="name">fleet.customer.version.view.form</field>
            <field name="model">fleet.customer.version</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="name"/>
                            <field name="model_id"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="fleet_customer_version_view_list" model="ir.ui.view">
            <field name="name">fleet.customer.version.view.list</field>
            <field name="model">fleet.customer.version</field>
            <field name="arch" type="xml">
                <list>
                    <field name="id"/>
                    <field name="name"/>
                    <field name="model_id"/>
                </list>
            </field>
        </record>

        <record id="fleet_customer_version_action" model="ir.actions.act_window">
            <field name="name">Versi√≥n</field>
            <field name="res_model">fleet.customer.version</field>
            <field name="view_mode">list,form</field>
            <field name="view_id" ref="fleet_customer_version_view_list"/>
        </record>
    </data>
</odoo>