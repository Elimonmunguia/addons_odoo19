<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="agenda_entrega_tipo_evento_view_form" model="ir.ui.view">
            <field name="name">agenda.entrega.tipo.evento.view.form</field>
            <field name="model">agenda.entrega.tipo.evento</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="name"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="agenda_entrega_tipo_evento_view_list" model="ir.ui.view">
            <field name="name">agenda.entrega.tipo.evento.view.list</field>
            <field name="model">agenda.entrega.tipo.evento</field>
            <field name="arch" type="xml">
               <list>
                   <field name="id"/>
                   <field name="name"/>
               </list>
            </field>
        </record>

        <record id="agenda_entrega_tipo_evento_action" model="ir.actions.act_window">
            <field name="name">Tipo de evento</field>
            <field name="res_model">agenda.entrega.tipo.evento</field>
            <field name="view_mode">list,form</field>
            <field name="view_id" ref="agenda_entrega_tipo_evento_view_list"/>
        </record>
    </data>
</odoo>