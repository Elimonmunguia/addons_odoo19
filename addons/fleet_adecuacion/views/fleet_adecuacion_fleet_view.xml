<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="fleet_adecuacion_inherit_view_form" model="ir.ui.view">
            <field name="name">fleet.adecuacion.inherit.view.form</field>
            <field name="model">fleet.vehicle</field>
            <field name="inherit_id" ref="fleet.fleet_vehicle_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//form/sheet/notebook" position="inside">
                    <page string="Adecuaciones" name="adecuaciones">
                        <field name="adecuacion_ids" />
                    </page>
                </xpath>
            </field>
        </record>
    </data>
</odoo>