<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="module_category_proveedor" model="ir.module.category">
            <field name="name">Proveedores</field>
            <field name="description">Gestión de acceso a proveedores</field>
        </record>

        <record id="group_manager" model="res.groups">
            <field name="name">Administrador de proveedores</field>
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
        </record>

        <record id="group_proveedor_administracion" model="res.groups">
            <field name="name">Administración - Contraloría </field>
        </record>

        <record id="group_proveedor_contabilidad" model="res.groups">
            <field name="name">Contabilidad </field>
        </record>

        <record id="group_proveedor_tesoreria" model="res.groups">
            <field name="name">Tesoreria</field>
        </record>
    </data>
</odoo>