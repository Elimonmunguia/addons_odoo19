<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="proveedor_tipo_view_form" model="ir.ui.view">
            <field name="name">proveedor.tipo.view.form</field>
            <field name="model">proveedor.tipo</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="name" required="1"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="proveedor_tipo_view_list" model="ir.ui.view">
            <field name="name">proveedor.tipo.view.list</field>
            <field name="model">proveedor.tipo</field>
            <field name="arch" type="xml">
                <list>
                    <field name="id"/>
                    <field name="name"/>
                </list>
            </field>
        </record>

        <record id="proveedor_tipo_action" model="ir.actions.act_window">
            <field name="name">Tipos de proveedor</field>
            <field name="res_model">proveedor.tipo</field>
            <field name="view_mode">list,form</field>
        </record>
    </data>
</odoo>