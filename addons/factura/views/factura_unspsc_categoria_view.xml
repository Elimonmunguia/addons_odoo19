<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="factura_unspsc_categoria_view_form" model="ir.ui.view">
            <field name="name">factura.unspsc.categoria.view.form</field>
            <field name="model">product.unspsc.categoria</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <field name="categoria_id" widget="many2one"/>
                        <field name="products_ids" widget="many2many"/>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="factura_unspsc_categoria_view_list" model="ir.ui.view">
            <field name="name">factura.unspsc.categoria.view.list</field>
            <field name="model">product.unspsc.categoria</field>
            <field name="arch" type="xml">
                <list>
                    <field name="categoria_id" widget="many2one"/>
                    <field name="products_ids" widget="many2many"/>
                </list>
            </field>
        </record>

        <record id="factura_unspsc_categoria_action" model="ir.actions.act_window">
            <field name="name">Categor√≠a - Unspsc</field>
            <field name="res_model">product.unspsc.categoria</field>
            <field name="view_mode">list,form</field>
            <field name="view_id" ref="factura_unspsc_categoria_view_list"/>
        </record>
    </data>
</odoo>