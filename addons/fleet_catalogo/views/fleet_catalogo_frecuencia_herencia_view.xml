<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="fleet_catalogo_frecuencia_inherit_poliza_view_form" model="ir.ui.view">
            <field name="name">fleet.catalogo.frecuencia.inherit.poliza.view.form</field>
            <field name="model">fleet.poliza</field>
            <field name="inherit_id" ref="fleet_poliza.fleet_poliza_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='prima_neta']" position="before">
                    <field name="frecuencia_pago_id"/>
                </xpath>
            </field>
        </record>

        <record id="fleet_catalogo_frecuencia_inherit_tramite_view_form" model="ir.ui.view">
            <field name="name">fleet.catalogo.frecuencia.inherit.tramite.view.form</field>
            <field name="model">fleet.tramite</field>
            <field name="inherit_id" ref="fleet_tramite.fleet_tramite_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='estado']" position="after">
                    <field name="frecuencia_pago_id" string="Periodicidad"/>
                </xpath>
            </field>
        </record>
    </data>


</odoo>